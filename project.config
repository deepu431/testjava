[access "refs/heads/*"]
	create = group user/deepu431
	push = +force group user/deepu431
	pushMerge = group user/deepu431
	read = group user/deepu431
	exclusiveGroupPermissions = create forgeAuthor forgeCommitter push pushMerge read
[access "refs/tags/*"]
	push = +force group user/deepu431
	exclusiveGroupPermissions = create push
