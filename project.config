[access "refs/*"]
	owner = group user/deepu431
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/deepu431
	label-Verified = -1..+1 group user/deepu431
	push = +force group user/deepu431
	read = group user/deepu431
	rebase = group user/deepu431
